
@{
    ViewBag.Title = "Create";
}
<div class="container">
    <form method="post" action="">
        <div class="form-group">
            <label for="name">Title</label>
            <input type="text" class="form-control" name="name" id="name" placeholder="Title">
        </div>
        <div class="form-group">
            <label for="x">Latitude</label>
            <input type="text" class="form-control" name="x" id="x" placeholder="Latitude">
        </div>
        <div class="form-group">
            <label for="y">Longitude</label>
            <input type="text" class="form-control" name="y" id="y" placeholder="Longitude">
        </div>
        <input type="submit" class="btn btn-default" value="Create" /> 
    </form>
 </div>

